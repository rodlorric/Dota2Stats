CREATE TABLE [dbo].[Ability_Upgrades](
[ability_upgrades_id] [bigint] IDENTITY(1,1) NOT NULL,
[match_id] [bigint] NOT NULL,
[player_slot] [smallint] NOT NULL,
[ability] [int] NOT NULL,
[time] [int] NOT NULL,
[level] [int] NOT NULL,
 CONSTRAINT [PK_ability_upgrades] PRIMARY KEY CLUSTERED

CREATE TABLE [dbo].[Match_Players](
[match_id] [bigint] NOT NULL,
[account_id] [bigint] NULL,
[player_slot] [smallint] NOT NULL,
[hero_id] [smallint] NOT NULL,
[item_0] [int] NULL,
[item_1] [int] NULL,
[item_2] [int] NULL,
[item_3] [int] NULL,
[item_4] [int] NULL,
[item_5] [int] NULL,
[kills] [int] NULL,
[deaths] [int] NULL,
[assists] [int] NULL,
[leaver_status] [int] NULL,
[gold] [int] NULL,
[last_hits] [int] NULL,
[denies] [int] NULL,
[gold_per_min] [int] NULL,
[xp_per_min] [int] NULL,
[gold_spent] [int] NULL,
[hero_damage] [int] NULL,
[tower_damage] [numeric](18, 0) NULL,
[hero_healing] [numeric](18, 0) NULL,
[level] [int] NULL,
 CONSTRAINT [PK_Match_Players] PRIMARY KEY CLUSTERED 
(
[match_id] ASC,
[player_slot] ASC
)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON) ON [PRIMARY]
) ON [PRIMARY]

CREATE TABLE [dbo].[Matches](
[radiant_win] [bit] NULL,
[duration] [int] NULL,
[start_time] [bigint] NOT NULL,
[start_time_DateTime]  AS (dateadd(second,[start_time],'19700101')),
[match_id] [bigint] NOT NULL,
[match_seq_num] [bigint] NOT NULL,
[tower_status_radiant] [int] NULL,
[tower_status_dire] [int] NULL,
[barracks_status_radiant] [int] NULL,
[barracks_status_dire] [int] NULL,
[cluster] [int] NULL,
[first_blood_time] [int] NULL,
[lobby_type] [int] NOT NULL,
[human_players] [smallint] NULL,
[leagueid] [int] NULL,
[positive_votes] [int] NULL,
[negative_votes] [int] NULL,
[game_mode] [int] NULL,
[FechaModificacion] [datetime] NOT NULL,
 CONSTRAINT [PK_Matches] PRIMARY KEY CLUSTERED 
(
[match_id] ASC
)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON) ON [PRIMARY]
) ON [PRIMARY]
